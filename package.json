{
  "name": "puppeteer",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "build:clean": "rm -rf dist && npm run build",
    "start": "npm run build && node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test:lambda": "npm run build && node test-lambda-local.js",
    "test:handler": "npm run build && ts-node -e \"import('./dist/handler').then(h => console.log('Handler loaded successfully'))\"",
    "deploy": "npm run build && serverless deploy",
    "deploy:dev": "npm run build && serverless deploy --stage dev",
    "deploy:prod": "npm run build && serverless deploy --stage prod",
    "local": "serverless offline",
    "invoke:local": "npm run build && serverless invoke local -f scrapeSponsored",
    "logs": "serverless logs -f scrapeSponsored",
    "logs:tail": "serverless logs -f scrapeSponsored --tail",
    "check": "tsc --noEmit",
    "test": "npm run check && npm run test:handler"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/aws-lambda": "^8.10.150",
    "@types/node": "^24.0.10",
    "puppeteer": "^24.11.2",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "serverless": "^3.40.0",
    "serverless-plugin-chrome": "^1.0.0-55.3",
    "serverless-plugin-typescript": "^2.1.5"
  }
}
